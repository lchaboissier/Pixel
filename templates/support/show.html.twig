{% extends 'back_layout.html.twig' %}

{% block title %}Détail du support{% endblock %}

{% block content %}
    <h1>Détail du support "{{ support.nom }}"</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ support.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ support.nom }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_support_index') }}" class="btn btn-primary">back to list</a>

    <a href="{{ path('app_support_edit', {'id': support.id}) }}" class="btn btn-primary">edit</a>

    {{ include('support/_delete_form.html.twig') }}
{% endblock %}